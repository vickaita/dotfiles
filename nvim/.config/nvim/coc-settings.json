{
  "diagnostic-languageserver.filetypes": {
    "vim": "vint",
    "sh": "shellcheck"
  },
  "languageserver": {
    "haskell": {
      "command": "hie-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "haskell": {
        }
      }
    }
  },
  "cSpell.userWords": [
    "Aita",
    "Frontmatter",
    "Vick",
    "nord",
    "spork",
    "struct"
  ]
}
